<%= simple_form_for(@setlist, method: :post, url: setlists_path) do |f| %>

  <%= f.input :name %>
  <%= f.file_field :setlist_image %>
  <%= f.button :submit, class: "btn-primary" %>
  <%= f.button :button, "Cancel", type: "reset", class: "btn-outline-secondary" %>
  <%= f.hidden_field :track, value: params[:spotify_id] %>
<% end %>
