<div class="container p-3">
  <h1>Stack</h1>
  <div class="dash-cards">
    <% if @stack.albums.empty? %>
      <p>Nothing in your Stack yet...</p>
    <% else %>
      <% @stack.stack_albums.each do |stack_album| %>
        <%= render 'shared/stack_album_card', album: stack_album.album %>
      <% end %>
    <% end %>
  </div>
</div>
